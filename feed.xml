---
layout: null
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Just Another DevBlog</title>
  <link href="http://tiffanywhite.tech"/>
  <link type="application/atom+xml" rel="self" href="http://example.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://tiffanywhite.tech/</id>
  <author>
    <name>Tiffany White</name>
    <email>hey@tiffanyrwhite.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>https://tiffanywhite.tech{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="https://tiffanywhite.tech{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Tiffany White</name>
      <uri>https://tiffanywhite.tech</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
